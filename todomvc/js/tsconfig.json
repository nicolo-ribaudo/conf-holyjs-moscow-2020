{
    "compilerOptions": {
        "target": "es2015",
        "isolatedModules": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "removeComments": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "../out",
        "lib": ["ES2015", "DOM"]
    },
    "files": [
        "constants.ts",
        "interfaces.d.ts",
        "todoModel.ts",
        "utils.ts",
        "app.tsx",
        "footer.tsx",
        "todoItem.tsx"
    ],
    "exclude": [],
    "baseUrl": "types",
    "typeRoots": ["types"],
}
